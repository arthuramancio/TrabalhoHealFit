<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo_mulher_emagrecimento"
    tools:context=".mulher_emagrecimento_nv2">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Space
            android:layout_width="match_parent"
            android:layout_height="20dp"/>

        <TextView
            android:id="@+id/textEmagrecimento"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#D5008545"
            android:gravity="center"
            android:padding="9dp"
            android:text="@string/emagrecimento"
            android:textColor="@color/white"
            android:textSize="24sp" />
        <Space
            android:layout_width="match_parent"
            android:layout_height="20dp" />

        <Button
            android:id="@+id/bttIntermediario"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/intermedi_rio"
            android:textSize="18sp"
            app:backgroundTint="#AE006A33" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="20dp" />

        <ImageView
            android:contentDescription="emaA"
            android:id="@+id/imageView16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/treino_mulher_emagrecimento_intermediario_a" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <ImageView
            android:contentDescription="emaB"
            android:id="@+id/imageView17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/treino_mulher_emagrecimento_intermediario_b" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <ImageView
            android:contentDescription="emaB"
            android:id="@+id/imageView18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/treino_mulher_emagrecimento_intermediario_c" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>